{
  "name": "genai-journal-website",
  "type": "module",
  "version": "0.1.0",
  "description": "Static website for GenAI Journal - Weekly curated AI and coding developments",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "clean": "rm -rf dist .astro",
    "astro": "astro",
    "check": "npm run type-check && npm run lint",
    "check:fix": "npm run format && npm run lint:fix && npm run type-check",
    "type-check": "astro check",
    "lint": "biome lint .",
    "lint:fix": "biome lint --apply .",
    "format": "prettier --write . && biome format --write .",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.5.0",
    "@types/dompurify": "^3.0.5",
    "@types/jsdom": "^21.1.7",
    "@types/marked": "^5.0.2",
    "astro": "^5.13.2",
    "dompurify": "^3.2.6",
    "jsdom": "^26.1.0",
    "marked": "^16.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@types/node": "^24.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "astro",
    "blog",
    "journal",
    "ai",
    "coding",
    "japanese",
    "static-site"
  ],
  "author": "GenAI Journal",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
